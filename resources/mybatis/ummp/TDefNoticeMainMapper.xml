  <sql id="queryNoticeMainInfoSQL">
		SELECT DISTINCT aa.id,aa.noticeTo "noticeTo"
    from (SELECT distinct
    na.id id,
    na.notice_id noticeId,
    tdu.email noticeTo
    from t_Def_Notice_Main na,t_def_user tdu
    where to_char(tdu.id)=na.notice_to) aa
    LEFT JOIN t_Def_Notice tDN ON
    tDN.NOTICE_ID=aa.noticeId
    LEFT JOIN t_def_monitor dm ON
    dm.monitor_code=tdn.notice_code
    where 1=1
    AND tdn.notice_code=#{monitorCode,jdbcType=VARCHAR}

    UNION

    SELECT DISTINCT bb.id,bb.noticeTo "noticeTo"
    from (SELECT distinct
    na.id id,
    na.notice_id noticeId,
    tda.account_addr noticeTo
    from t_Def_Notice_Main na,t_def_notice_account tda
    where to_char(tda.id)=na.notice_to) bb
    LEFT JOIN t_Def_Notice tDN ON
    tDN.NOTICE_ID=bb.noticeId
    LEFT JOIN t_def_monitor dm ON
    dm.monitor_code=tdn.notice_code
    where 1=1
    AND
    tdn.notice_code=#{monitorCode,jdbcType=VARCHAR}
	</sql>
