		DS.SQL_CODE "sqlCode",
		DS.SQL_DESC "sqlDesc",
		NVL((CASE be.es_Status WHEN 1 THEN '有效' WHEN 2 THEN '删除' ELSE '无效'
		END),be.es_Status) as "esStatus"
		FROM T_DEF_BAG_EVENT BE
		LEFT JOIN T_DEF_SQL DS
		ON BE.SQL_ID = DS.SQL_ID
		LEFT JOIN T_DEF_MONITOR MN
		ON BE.MONITOR_CODE = MN.MONITOR_CODE
		WHERE 1=1
