	<!-- 查询详细信息 -->
	<sql id="queryCodedictListInfoSQL">
		select tdcd.code_type "codetype", tdcd.code "code", tdcd.code_name
		"codename", tdcd.code_alias "codealias",
		 tdcd.com_id "comid",
		(select plcom.com_name from t_def_plat_com plcom where plcom.com_id=tdcd.com_id) "comidname", 
		 tdcd.es_status "esstatus",
		(select code_name from t_def_code_dict where code_type = 'ES_STATUS' and code = tdcd.es_status) "esstatus1" 
		from t_def_code_dict tdcd where 1=1
		<if test="codetype!=null and codetype!=''"> and tdcd.code_type like '%' || #{codetype} || '%' </if>

		<if test="code!=null and code!=''"> and tdcd.code like '%' || #{code} || '%' </if>

		<if test="codename!=null and codename!=''"> and tdcd.code_name like '%' || #{codename} || '%' </if>

		<if test="codealias!=null and codealias!=''"> and tdcd.code_alias like '%' || #{codealias} || '%' </if>

		<if test="comid!=null and comid!=''"> and tdcd.com_id like '%' || #{comid} || '%' </if>

		<if test="esstatus!=null and esstatus!=''"> and tdcd.es_status like '%' || #{esstatus} || '%' </if>
		order by tdcd.code_type
	</sql>

