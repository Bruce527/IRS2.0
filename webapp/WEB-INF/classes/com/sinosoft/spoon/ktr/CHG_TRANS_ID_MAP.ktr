    <sql>select pc.policy_chg_id as chgid,&#xd;&#xa;       &#x27;000031&#x27; &#x7c;&#x7c; to_char&#x28;sysdate, &#x27;yyyymmdd&#x27;&#x29; &#x7c;&#x7c;&#xd;&#xa;       lpad&#x28;&#x28;&#x24;&#x7b;maxNo&#x7d; &#x2b; rownum&#x29;, &#x27;10&#x27;, &#x27;0&#x27;&#x29; as transno,&#xd;&#xa;       p.policy_no as bussno,&#xd;&#xa;       &#x27;P&#x27; as busstype,&#xd;&#xa;       to_char&#x28;sysdate, &#x27;yyyymmdd&#x27;&#x29; as dateserial,&#xd;&#xa;       &#x27;000031&#x27; as companycode&#xd;&#xa;  from ods_stg.OFE_T_POLICY_CHANGE pc, ods_stg.OFE_T_POLICY p&#xd;&#xa; where pc.policy_id &#x3d; p.policy_id&#xd;&#xa;   and pc.rc_state &#x3d; &#x27;E&#x27;&#xd;&#xa;   and p.rc_state &#x3d; &#x27;E&#x27;&#xd;&#xa;   and &#x28;p.policy_no &#x3d; &#x27;&#x24;&#x7b;bussNo&#x7d;&#x27; or &#x27;BUSSNOBATCH&#x27; &#x3d; &#x27;&#x24;&#x7b;bussNoBatch&#x7d;&#x27;&#x29;&#xd;&#xa;   and pc.item_id in &#x28;&#x24;&#x7b;bussTypeIn&#x7d;&#x29;&#xd;&#xa;   and pc.effective_date between to_date&#x28;&#x27;&#x24;&#x7b;startDate&#x7d;&#x27;, &#x27;yyyy-mm-dd&#x27;&#x29; and to_date&#x28;&#x27;&#x24;&#x7b;endDate&#x7d;&#x27;, &#x27;yyyy-mm-dd&#x27;&#x29;&#xd;&#xa;   and not exists &#x28;select 1 from ods_stg.OFE_T_RLS_POLICY where PNO &#x3d; p.policy_no&#x29;&#xd;&#xa;   and exists&#xd;&#xa;       &#x28;select TTPL.POLICY_ID&#xd;&#xa;          from ods_stg.ofe_T_PD_PRODUCT TTPD, ODS_STG.OFE_T_POL_PLAN TTPL&#xd;&#xa;         WHERE basic_product_flag &#x3d; &#x27;B&#x27;&#xd;&#xa;           AND Coverage_Term_Type in &#x28;&#x27;L&#x27;, &#x27;M&#x27;&#x29;&#xd;&#xa;           AND TTPD.PD_PRODUCT_ID &#x3d; TTPL.PD_PRODUCT_ID&#xd;&#xa;           AND TTPD.RC_STATE &#x3d; &#x27;E&#x27;&#xd;&#xa;           and TTPL.Rc_State &#x3d; &#x27;E&#x27;&#xd;&#xa;           and &#x28;ttpl.plan_terminated_date &#x3e; date&#xd;&#xa;                &#x27;2016-01-01&#x27; or ttpl.plan_terminated_date is null or&#xd;&#xa;                ttpl.plan_terminated_date &#x3d; &#x27;&#x27;&#x29;&#xd;&#xa;           and pc.policy_id &#x3d; ttpl.policy_id&#x29;</sql>
